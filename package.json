{
  "name": "@prxm/clickhouse",
  "version": "0.0.0-development",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/proximahq/clickhouse.git"
  },
  "source": "src/index.ts",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "exports": {
    "require": "./dist/index.js",
    "default": "./dist/index.mjs"
  },
  "types": "dist/index.d.ts",
  "files": [
    "dist/**.js*",
    "dist/**/*.ts"
  ],
  "scripts": {
    "build": "tsup --env.NODE_ENV production ./src/index.ts --dts --format esm,cjs --outDir dist",
    "watch": "tsup --env.NODE_ENV production ./src/index.ts --format esm,cjs --watch --outDir dist",
    "test": "vitest run",
    "test:debug": "DEBUG=proxima:* vitest run",
    "prepare": "husky install",
    "prepack": "rm -rf dist && npm run build",
    "semantic-release": "semantic-release"
  },
  "dependencies": {
    "debug": "4.3.4",
    "lodash": "4.17.21",
    "nanoid": "3.3.4",
    "sqlstring": "2.3.3",
    "undici": "5.20.0"
  },
  "devDependencies": {
    "@commitlint/cli": "13.1.0",
    "@commitlint/config-conventional": "13.1.0",
    "@types/debug": "^4.1.7",
    "@types/node": "16.10.3",
    "@types/sqlstring": "^2.3.0",
    "eslint": "7.17.0",
    "husky": "^7.0.2",
    "lint-staged": "10.3.0",
    "node-fetch": ">= 2.6.3",
    "prettier": "2.6.2",
    "semantic-release": "19.0.2",
    "tsup": "5.12.7",
    "vite-plugin-vitest-typescript-assert": "^1.1.4",
    "vitest": "0.12.2"
  },
  "license": "MIT",
  "engines": {
    "node": ">=14"
  },
  "directories": {
    "test": "tests"
  },
  "release": {
    "branches": [
      "main"
    ]
  }
}
